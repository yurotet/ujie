<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="router">
                    <match url="webapp/.*" />
                    <action type="Rewrite" url="/res/index.html" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
